{"ast":null,"code":"import TopToolbar from \"@/components/users/userToolbar.vue\";\nimport userFooter from \"@/components/users/userFooter.vue\";\nimport userMap from \"@/components/users/userMap.vue\";\nimport axios from \"axios\";\nexport default {\n  name: 'HomeView',\n  data: () => {\n    return {\n      maneges: [],\n      stands: []\n    };\n  },\n  components: {\n    TopToolbar,\n    userFooter,\n    userMap\n  },\n  methods: {\n    getData() {\n      axios({\n        method: 'get',\n        url: 'http://localhost:3000/'\n      }).then(data => {\n        if (data.data.success) {\n          this.maneges = data.data.data.maneges;\n          this.stands = data.data.data.stands;\n        }\n      });\n    }\n  },\n  mounted() {\n    this.getData();\n  }\n};","map":{"version":3,"mappings":"AA8BA;AACA;AACA;AACA;AAEA;EACAA;EACAC;IACA;MACAC;MACAC;IACA;EACA;EACAC;IACAC;IACAC;IACAC;EACA;EACAC;IACAC;MACAC;QACAC;QACAC;MACA;QACA;UACA;UACA;QACA;MACA;IACA;EACA;EACAC;IACA;EACA;AACA","names":["name","data","maneges","stands","components","TopToolbar","userFooter","userMap","methods","getData","axios","method","url","mounted"],"sourceRoot":"src/views","sources":["Home.vue"],"sourcesContent":["<template>\n  <div>\n    <header>\n      <top-toolbar></top-toolbar>\n    </header>\n\n   <div style=\"height: 100vh; background-image: url('https://vivreparis.fr/wp-content/uploads/2021/11/carrousel-paris-noel.jpg'); background-size: cover\">\n      <div style=\"color: white; height: 50vh; width: 80vw; position: absolute;  margin: 25vh 10vw; font-size: 2.5vw;\">\n        <h2>{{ $t('home.title') }}</h2>\n        <h1>{{$t('attribute.from')}} {{this.$store.state.manifestation.datedebut}} {{$t('attribute.to')}} {{this.$store.state.manifestation.datefin}}</h1>\n      </div>\n   </div>\n\n    <div id=\"presentation\">\n      <div style=\"margin:30vh 10%; background-color: #3f4545; color: white; padding: 50px\">\n        <h3 style=\"text-align: justify;font-size: 4vh\">{{$t('home.description')}