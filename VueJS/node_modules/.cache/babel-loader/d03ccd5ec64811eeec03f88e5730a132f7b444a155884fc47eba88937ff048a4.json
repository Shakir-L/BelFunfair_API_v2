{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  name: \"prestataireShowManeges\",\n  data: () => {\n    return {\n      id_user: null,\n      maneges: []\n    };\n  },\n  methods: {\n    add() {\n      this.$router.replace('/prestataire/maneges/add');\n    },\n    update(id) {\n      this.$router.replace('/prestataire/maneges/' + id);\n    },\n    _delete(id) {\n      axios({\n        method: 'post',\n        url: 'http://localhost:3000/prestataires/' + this.id_user + '/manege/' + id + '/delete'\n      }).then(result => {\n        this.$store.commit('setMessage', result.data);\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n    signup(id) {\n      this.$router.replace('/prestataire/maneges/' + id + '/signup');\n    }\n  },\n  mounted() {\n    this.id_user = this.$store.state.user.id;\n    axios({\n      method: 'get',\n      url: 'http://localhost:3000/prestataires/' + this.id_user + '/manege'\n    }).then(result => {\n      this.maneges = result.data;\n    }).catch(error => {\n      console.log(error);\n    });\n  }\n};","map":{"version":3,"mappings":"AAgCA;AAEA;EACAA;EACAC;IACA;MACAC;MACAC;IACA;EACA;EACAC;IACAC;MAAA;IAAA;IACAC;MAAA;IAAA;IACAC;MACAC;QACAC;QACAC;MACA;QACA;MACA;QACAC;MACA;IACA;IACAC;MAAA;IAAA;EACA;EACAC;IACA;IACAL;MACAC;MACAC;IACA;MACA;IACA;MACAC;IACA;EACA;AACA","names":["name","data","id_user","maneges","methods","add","update","_delete","axios","method","url","console","signup","mounted"],"sourceRoot":"src/components/prestataires/manege","sources":["prestataireShowManeges.vue"],"sourcesContent":["<template>\r\n  <div style=\"margin: 5%\">\r\n    <v-btn dark @click=\"add()\">{{$t('button.add')}}</v-btn>\r\n    <div v-for=\"(manege,index) in maneges\" :key=\"'prestataire-manege-'+index\">\r\n      <div style=\"background-color: #3f4545; color: white; margin: 2%; padding: 2%\">\r\n        <h1>{{manege.name}}</h1>\r\n\r\n        <div style=\"text-align: center\">\r\n          <h4 v-if=\"manege.status === 'attributed'\" style=\"color: green\">{{$t('status.attributed')}}</h4>\r\n          <h4 v-if=\"manege.status === 'wait_attribution'\" style=\"color: orange\">{{$t('status.wait_attribution')}}</h4>\r\n          <h4 v-if=\"manege.status === 'not_attributed'\" style=\"color: red\">{{$t('status.not_attributed')}}</h4>\r\n        </div>\r\n\r\n        <h2>- {{manege.type}} -</h2>\r\n\r\n        <h3 v-if=\"manege.taille_min\">{{$t('attribute.taille')}}: {{manege.taille_min}}</h3>\r\n\r\n        <p style=\"text-align: justify; padding: 2%\">{{manege.description}}</p>\r\n\r\n        <v-btn fab small dark color=\"blue\" v-if=\"manege.status === 'not_attributed'\" @click=\"update(manege.id)\"><v-icon>mdi-pencil</v-icon></v-btn>\r\n        <v-btn fab small @click=\"signup(manege.id)\"><v-icon>mdi-format-list-bulleted-square</v-icon></v-btn>\r\n        <v-btn fab small outlined color=\"red\" @click=\"_delete(manege.id)\"><v-icon>mdi-delete</v-icon></v-btn>\r\n\r\n        <div v-for=\"(image,index) in manege.images\" :key=\"'prestataire-manege-image-'+index\">\r\n          <img :src=\"image\">\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"prestataireShowManeges\",\r\n  data:()=>{\r\n    return{\r\n      id_user:null,\r\n      maneges:[]\r\n    }\r\n  },\r\n  methods:{\r\n    add(){this.$router.replace('/prestataire/maneges/add')},\r\n    update(id){this.$router.replace('/prestataire/maneges/'+id)},\r\n    _delete(id){\r\n      axios({\r\n        method: 'post',\r\n        url: 'http://localhost:3000/prestataires/'+this.id_user+'/manege/'+id+'/delete'\r\n      }).then(result=>{\r\n        this.$store.commit('setMessage',result.data)\r\n      }).catch(error=>{\r\n        console.log(error)\r\n      })\r\n    },\r\n    signup(id){this.$router.replace('/prestataire/maneges/'+id+'/signup')}\r\n  },\r\n  mounted(){\r\n    this.id_user = this.$store.state.user.id\r\n    axios({\r\n      method: 'get',\r\n      url: 'http://localhost:3000/prestataires/'+this.id_user+'/manege'\r\n    }).then(result=>{\r\n      this.maneges = result.data\r\n    }).catch(error=>{\r\n      console.log(error)\r\n    })\r\n  }\r\n}\r\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}