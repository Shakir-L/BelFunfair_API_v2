{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  name: \"prestataireShowManeges\",\n  data: () => {\n    return {\n      maneges: []\n    };\n  },\n  mounted() {\n    let id_user = this.$store.state.user.id;\n    axios({\n      method: 'get',\n      url: 'http://localhost:3000/prestataires/' + id_user + '/manege'\n    }).then(result => {\n      this.maneges = result.data;\n    }).catch(error => {\n      console.log(error);\n    });\n  }\n};","map":{"version":3,"mappings":"AAiBA;AAEA;EACAA;EACAC;IACA;MACAC;IACA;EACA;EACAC;IACA;IACAC;MACAC;MACAC;IACA;MACA;IACA;MACAC;IACA;EACA;AACA","names":["name","data","maneges","mounted","axios","method","url","console"],"sourceRoot":"src/components/prestataires","sources":["prestataireShowManeges.vue"],"sourcesContent":["<template>\r\n  <div style=\"margin: 2%; padding: 2%\">\r\n    <div v-for=\"(manege,index) in maneges\" :key=\"'prestataire-manege-'+index\">\r\n      <div style=\"background-color: #3f4545; color: white; margin: 2%; padding: 2%\">\r\n        <h1>{{manege.name}}</h1>\r\n        <h2>- {{manege.type}} -</h2>\r\n        <h3 v-if=\"manege.taille_min\">Taille minimale: {{manege.taille_min}}cm</h3>\r\n        <p style=\"text-align: justify; padding: 2%\">{{manege.description}}</p>\r\n        <div v-for=\"(image,index) in manege.images\" :key=\"'prestataire-manege-image-'+index\">\r\n          <img :src=\"image\">\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"prestataireShowManeges\",\r\n  data:()=>{\r\n    return{\r\n      maneges:[]\r\n    }\r\n  },\r\n  mounted(){\r\n    let id_user = this.$store.state.user.id\r\n    axios({\r\n      method: 'get',\r\n      url: 'http://localhost:3000/prestataires/'+id_user+'/manege'\r\n    }).then(result=>{\r\n      this.maneges = result.data\r\n    }).catch(error=>{\r\n      console.log(error)\r\n    })\r\n  }\r\n}\r\n</script>"]},"metadata":{},"sour