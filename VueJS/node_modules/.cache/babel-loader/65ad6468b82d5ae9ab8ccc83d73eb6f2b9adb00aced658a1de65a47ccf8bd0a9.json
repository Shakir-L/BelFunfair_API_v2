{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  name: \"prestataireUpdateManege\",\n  data: () => {\n    return {\n      id_user: null,\n      id: null,\n      manege: null,\n      types: null,\n      name: null,\n      description: null,\n      type: null,\n      taille: null\n    };\n  },\n  mounted() {\n    this.id_user = this.$store.state.user.id;\n    this.id = this.$route.params.id;\n    axios({\n      method: 'get',\n      url: 'http://localhost:3000/prestataires/' + this.id_user + '/manege/' + this.id\n    }).then(result => {\n      this.manege = result.data.data.manege;\n      this.types = result.data.data.types;\n      this.name = this.manege.name;\n      this.description = this.manege.description;\n      this.type = this.manege.type;\n      this.taille = this.manege.taille_min;\n    }).catch(error => {\n      console.log(error);\n    });\n  }\n};","map":{"version":3,"mappings":"AA0BA;AAEA;EACAA;EACAC;IACA;MACAC;MACAC;MACAC;MACAC;MACAL;MACAM;MACAC;MACAC;IACA;EACA;EACAC;IACA;IACA;IACAC;MACAC;MACAC;IACA;MACA;MACA;MACA;MACA;MACA;MACA;IACA;MACAC;IACA;EACA;AACA","names":["name","data","id_user","id","manege","types","description","type","taille","mounted","axios","method","url","console"],"sourceRoot":"src/components/prestataires","sources":["prestataireUpdateManege.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div>\r\n      <h1>{{this.manege.name}}</h1>\r\n      <form>\r\n        <h4>{{ $t('attribute.name') }}</h4>\r\n        <input v-model=\"name\" type=\"text\" required />\r\n\r\n        <h4>{{ $t('attribute.description') }}</h4>\r\n        <v-textarea style=\"background-color: white\" v-model=\"description\" type=\"text\" required />\r\n\r\n        <v-select v-for=\"(type,index) in types\" :key=\"'prestataire-update-type-'+index\">\r\n          <option>{{type.name}}</option>\r\n        </v-select>\r\n\r\n        <h4>{{ $t('attribute.type') }}</h4>\r\n        <input v-model=\"type\" type=\"text\" required />\r\n\r\n        <h4>{{ $t('attribute.taille') }}</h4>\r\n        <input v-model=\"taille\" type=\"text\" required />\r\n      </form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"prestataireUpdateManege\",\r\n  data:()=>{\r\n    return{\r\n      id_user:null,\r\n      id:null,\r\n      manege:null,\r\n      types:null,\r\n      name:null,\r\n      description:null,\r\n      type:null,\r\n      taille:null\r\n    }\r\n  },\r\n  mounted(){\r\n    this.id_user = this.$store.state.user.id\r\n    this.id = this.$route.params.id\r\n    axios({\r\n      method: 'get',\r\n      url: 'http://localhost:3000/prestataires/'+this.id_user+'/manege/'+this.id\r\n    }).then(result=>{\r\n      this.manege = result.data.data.manege\r\n      this.types = result.data.data.types\r\n      this.name = this.manege.name\r\n      this.description = this.manege.description\r\n      this.type = this.manege.type\r\n      this.taille = this.manege.taille_min\r\n    }).catch(error=>{\r\n      console.log(error)\r\n    })\r\n  }\r\n}\r\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}