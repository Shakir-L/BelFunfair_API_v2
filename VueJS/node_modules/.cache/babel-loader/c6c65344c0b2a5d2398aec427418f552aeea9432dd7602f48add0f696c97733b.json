ab>\r\n      </v-tabs>\r\n    </template>\r\n\r\n  </v-app-bar>\r\n</template>\r\n\r\n<script>\r\nimport i18n from \"@/i18n\";\r\n\r\nexport default {\r\n  name: 'topToolbar',\r\n  data:()=>({\r\n  }),\r\n  methods: {\r\n    goTo (path) {this.$router.replace(path)},\r\n    changeLanguage(lang){return i18n.locale = lang}\r\n  }\r\n}\r\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}