{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  name: \"userAttractions\",\n  data: () => {\n    return {\n      maneges: null,\n      stands: null,\n      artistes: null\n    };\n  },\n  methods: {\n    getData() {\n      axios({\n        method: 'get',\n        url: 'http://localhost:3000/'\n      }).then(data => {\n        if (data.data.success) {\n          this.maneges = data.data.data.maneges;\n          this.stands = data.data.data.stands;\n          this.artistes = data.data.data.artistes;\n        }\n      });\n    },\n    showManeges() {\n      let show = this.$t('attribute.show');\n      document.getElementById(\"btnManege\").innerText = show;\n    }\n  },\n  mounted() {\n    this.getData();\n  }\n};","map":{"version":3,"mappings":"AAqBA;AAEA;EACAA;EACAC;IACA;MACAC;MACAC;MACAC;IACA;EACA;EACAC;IACAC;MACAC;QACAC;QACAC;MACA;QACA;UACA;UACA;UACA;QACA;MACA;IACA;IACAC;MACA;MACAC;IACA;EACA;EACAC;IAAA;EAAA;AACA","names":["name","data","maneges","stands","artistes","methods","getData","axios","method","url","showManeges","document","mounted"],"sourceRoot":"src/components/users","sources":["userAttractions.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <h1>{{$t('attribute.manege')}}</h1>\r\n    <v-btn v-on:click=\"showManeges\" id=\"btnManege\"></v-btn>\r\n    <div v-for=\"(m,i) in maneges\" :key=\"'attractions-manege-'+i\">\r\n      <div style=\"width: 80vw; background-color: #cccccc; border-radius: 10px; padding: 10px; margin: 1vh 10vw\">\r\n        <h2>{{m.name}}</h2>\r\n        <h3>{{$t('attribute.from')}} {{m.datedebut}} {{$t('attribute.to')}} {{m.datefin}}</h3>\r\n        <ul>\r\n          <li>{{$t('attribute.type')}}: {{m.type}}</li>\r\n          <li v-if=\"m.taille_min\">{{$t('attribute.taille')}}: {{m.taille_min}}</li>\r\n        </ul>\r\n        <div style=\"background-color: white; border-radius: 10px; padding: 10px; text-align: justify; margin: 10px\">\r\n          {{m.description}}\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"userAttractions\",\r\n  data:()=>{\r\n    return{\r\n      maneges:null,\r\n      stands:null,\r\n      artistes:null\r\n    }\r\n  },\r\n  methods:{\r\n    getData(){\r\n      axios({\r\n        method: 'get',\r\n        url: 'http://localhost:3000/'\r\n      }).then(data=>{\r\n        if(data.data.success){\r\n          this.maneges = data.data.data.maneges\r\n          this.stands = data.data.data.stands\r\n          this.artistes = data.data.data.artistes\r\n        }\r\n      })\r\n    },\r\n    showManeges(){\r\n      let show = this.$t('attribute.show')\r\n      document.getElementById(\"btnManege\").innerText = show\r\n    }\r\n  },\r\n  mounted() {this.getData()}\r\n}\r\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}