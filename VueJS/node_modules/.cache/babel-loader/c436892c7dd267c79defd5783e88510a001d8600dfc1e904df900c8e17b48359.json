{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  name: \"prestataireAddManege\",\n  data: () => {\n    return {\n      name: null,\n      description: null,\n      type: null,\n      taille: null,\n      id_user: null,\n      types: null,\n      message: null\n    };\n  },\n  methods: {\n    add() {\n      let id_type;\n      for (let i; i < this.types.length; i++) {\n        if (this.types[i].libelle === this.type) id_type = this.types[i].id;\n      }\n      console.log(id_type);\n      axios({\n        method: 'post',\n        url: 'http://localhost:3000/prestataires/' + this.id_user + '/addManege',\n        data: {\n          name: this.name,\n          description: this.description,\n          type: id_type,\n          taille_min: this.taille\n        }\n      }).then(result => {\n        this.message = result.data.data;\n        let color = 'red';\n        if (result.data.success) color = 'green';\n        document.querySelector('#message').style.backgroundColor = color;\n      }).catch(error => {\n        this.message = error;\n        document.querySelector('#message').style.backgroundColor = 'red';\n      });\n    }\n  },\n  mounted() {\n    this.id_user = this.$store.state.user.id;\n    axios({\n      method: 'get',\n      url: 'http://localhost:3000/users/types'\n    }).then(result => {\n      this.types = result.data.data;\n    }).catch(error => {\n      console.log(error);\n    });\n  }\n};","map":{"version":3,"mappings":"AA8BA;AAEA;EACAA;EACAC;IACA;MACAD;MACAE;MACAC;MACAC;MACAC;MACAC;MACAC;IACA;EACA;EACAC;IACAC;MACA;MACA;QACA;MACA;MACAC;MACAC;QACAC;QACAC;QACAZ;UACAD;UACAE;UACAC;UACAW;QACA;MACA;QACA;QACA;QACA;QACAC;MACA;QACA;QACAA;MACA;IACA;EACA;EACAC;IACA;IACAL;MACAC;MACAC;IACA;MACA;IACA;MACAH;IACA;EACA;AACA","names":["name","data","description","type","taille","id_user","types","message","methods","add","console","axios","method","url","taille_min","document","mounted"],"sourceRoot":"src/components/prestataires","sources":["prestataireAddManege.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div id=\"message\" style=\"color: white; text-align: center\">\r\n      {{this.message}}\r\n    </div>\r\n    <div style=\"margin: 10%; padding: 3%; background-color: #3f4545\">\r\n      <h1 style=\"color: white\">{{$t('manege.add')}}</h1>\r\n      <form>\r\n        <h4>{{ $t('attribute.name') }}</h4>\r\n        <input v-model=\"name\" type=\"text\" required />\r\n\r\n        <h4>{{ $t('attribute.description') }}</h4>\r\n        <input v-model=\"description\" type=\"text\" required />\r\n\r\n        <h4>{{ $t('attribute.type') }}</h4>\r\n        <select v-model=\"type\" style=\"background-color: white; width: 100%\">\r\n          <option v-for=\"(type,index) in types\" :key=\"'prestataire-add-type-'+index\">\r\n            {{type.libelle}}\r\n          </option>\r\n        </select>\r\n\r\n        <h4>{{ $t('attribute.taille') }}</h4>\r\n        <input v-model=\"taille\" type=\"number\" required />\r\n        <v-btn class=\"btn btn-dark\" type=\"button\" v-on:click=\"add()\">{{ $t('button.add') }}</v-btn>\r\n      </form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"prestataireAddManege\",\r\n  data:()=>{\r\n    return{\r\n      name:null,\r\n      description:null,\r\n      type:null,\r\n      taille:null,\r\n      id_user:null,\r\n      types:null,\r\n      message:null\r\n    }\r\n  },\r\n  methods:{\r\n    add(){\r\n      let id_type\r\n      for(let i; i<this.types.length; i++){\r\n        if(this.types[i].libelle === this.type) id_type = this.types[i].id\r\n      }\r\n      console.log(id_type)\r\n      axios({\r\n        method: 'post',\r\n        url: 'http://localhost:3000/prestataires/'+this.id_user+'/addManege',\r\n        data: {\r\n          name: this.name,\r\n          description: this.description,\r\n          type: id_type,\r\n          taille_min: this.taille\r\n        }\r\n      }).then(result=>{\r\n        this.message = result.data.data\r\n        let color = 'red'\r\n        if(result.data.success) color = 'green'\r\n        document.querySelector('#message').style.backgroundColor = color\r\n      }).catch(error=>{\r\n        this.message = error\r\n        document.querySelector('#message').style.backgroundColor = 'red'\r\n      })\r\n    }\r\n  },\r\n  mounted(){\r\n    this.id_user = this.$store.state.user.id\r\n    axios({\r\n      method: 'get',\r\n      url: 'http://localhost:3000/users/types'\r\n    }).then(result=>{\r\n      this.types = result.data.data\r\n    }).catch(error=>{\r\n      console.log(error)\r\n    })\r\n  }\r\n}\r\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}