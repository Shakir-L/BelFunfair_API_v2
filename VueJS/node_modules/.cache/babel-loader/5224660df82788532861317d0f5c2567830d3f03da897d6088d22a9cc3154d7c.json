{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  name: \"userLogin\",\n  data: () => ({\n    firstname: null,\n    surname: null,\n    password: null\n  }),\n  methods: {\n    login() {\n      axios({\n        method: 'post',\n        url: 'http://localhost:3000/users/login',\n        data: {\n          firstname: this.firstname,\n          surname: this.surname,\n          password: this.password\n        }\n      }).then(result => {\n        let data = result.data.data;\n        console.log(data);\n        this.$store.commit('setMessage', {\n          success: data.success,\n          data: data.message\n        });\n        if (data.data.length !== 0) {\n          this.$store.commit('setUser', data.data);\n          this.$router.replace('/' + data.data.role);\n        }\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n  },\n  mounted() {\n    let user = this.$store.state.user;\n    if (user) this.$router.replace('/' + user.role);\n  }\n};","map":{"version":3,"mappings":"AAqBA;AAEA;EACAA;EACAC;IACAC;IACAC;IACAC;EACA;EACAC;IACAC;MACAC;QACAC;QACAC;QACAR;UACAC;UACAC;UACAC;QACA;MACA;QACA;QACAM;QACA;UAAAC;UAAAV;QAAA;QACA;UACA;UACA;QACA;MACA;QACAS;MACA;IACA;EACA;EACAE;IACA;IACA;EACA;AACA","names":["name","data","firstname","surname","password","methods","login","axios","method","url","console","success","mounted"],"sourceRoot":"src/components/users","sources":["userLogin.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <h1>{{ $t('button.login') }}</h1>\r\n    <h3>{{ $t('login.description') }}</h3>\r\n\r\n    <form>\r\n      <h4>{{ $t('attribute.firstname') }}</h4>\r\n      <input v-model=\"firstname\" type=\"text\" required />\r\n\r\n      <h4>{{ $t('attribute.surname') }}</h4>\r\n      <input v-model=\"surname\" type=\"text\" required />\r\n\r\n      <h4>{{ $t('attribute.password') }}</h4>\r\n      <input v-model=\"password\" type=\"password\" required />\r\n\r\n      <v-btn class=\"btn btn-dark\" type=\"button\" v-on:click=\"login\">{{ $t('button.login') }}</v-btn>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"userLogin\",\r\n  data:()=>({\r\n    firstname:null,\r\n    surname:null,\r\n    password:null\r\n  }),\r\n  methods:{\r\n    login(){\r\n      axios({\r\n        method: 'post',\r\n        url: 'http://localhost:3000/users/login',\r\n        data: {\r\n          firstname: this.firstname,\r\n          surname: this.surname,\r\n          password: this.password\r\n        }\r\n      }).then(result=>{\r\n        let data = result.data.data\r\n        console.log(data)\r\n        this.$store.commit('setMessage', {success:data.success,data:data.message})\r\n        if(data.data.length !== 0){\r\n          this.$store.commit('setUser',data.data)\r\n          this.$router.replace('/'+data.data.role)\r\n        }\r\n      }).catch(error=>{\r\n        console.log(error)\r\n      })\r\n    }\r\n  },\r\n  mounted(){\r\n    let user = this.$store.state.user\r\n    if(user) this.$router.replace('/'+user.role)\r\n  }\r\n}\r\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}